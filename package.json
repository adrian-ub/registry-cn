{
  "name": "registry-cn",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.14.4",
  "description": "Generate registry for cn components",
  "author": "Adri√°n UB",
  "license": "MIT",
  "funding": "https://github.com/sponsors/adrian-ub",
  "homepage": "https://github.com/adrian-ub/registry-cn#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrian-ub/registry-cn.git"
  },
  "bugs": "https://github.com/adrian-ub/registry-cn/issues",
  "keywords": [
    "registry",
    "shadcn",
    "plugin"
  ],
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "registry-cn": "bin/registry-cn.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "tsx src/cli.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs": "nr -C docs dev",
    "docs:build": "nr -C docs build"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.0",
    "deepmerge": "^4.3.1",
    "destr": "^2.0.3",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "picocolors": "^1.1.1",
    "ts-morph": "^25.0.0",
    "unconfig": "^0.6.0",
    "valibot": "1.0.0-beta.9",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@adrianub/eslint-config": "^0.0.4",
    "@types/node": "^22.10.5",
    "@types/yargs": "^17.0.33",
    "astro-eslint-parser": "^1.1.0",
    "bumpp": "^9.9.2",
    "eslint": "^9.15.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.11",
    "prettier-plugin-astro": "^0.14.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^3.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
